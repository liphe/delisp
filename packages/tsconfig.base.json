{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["ES2019"],
    "module": "commonjs",
    "strict": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "composite": true,
    "sourceMap": true
  }
}
